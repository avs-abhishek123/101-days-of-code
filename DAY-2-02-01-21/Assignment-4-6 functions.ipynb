    "def computepay(h,r):\n",
    "    if(h<=40.0):\n",
    "      pay= h*r\n",
    "    elif(h>r):\n",
    "      pay=(((40.0)*r)+((h-40.0)*(r*1.5)))\n",
    "    return(pay)\n",
    "hrs = input(\"Enter Hours:\")\n",
    "h = float(hrs)\n",
    "rate = input(\"Enter the rate\")\n",
    "r= float(rate)\n",
    "\n",
    "print(computepay(h,r))\n",
    " "
